{
  "name": "lilfetch",
  "version": "1.0.0",
  "description": "MCP server for scraping webpages to clean Markdown, wrapped for easy npx use.",
  "main": "bin/lilfetch.js",
  "bin": {
    "lilfetch": "./bin/lilfetch.js"
  },
  "scripts": {
    "postinstall": "node install-python-deps.js",
    "pack": "npm pack",
    "dev": "node bin/lilfetch.js",
    "global-install": "npm install -g .",
    "global-uninstall": "npm uninstall -g lilfetch && rm -rf ~/.lilfetch-venv"
  },
  "keywords": ["mcp", "scraper", "markdown", "web"],
  "author": "jphdevsf",
  "license": "MIT",
  "dependencies": {
    "child_process": "^1.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}